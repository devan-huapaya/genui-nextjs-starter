{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src"],
		"includePattern": "\\.(jsx|js|ts|tsx)$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"better-docs/component",
		"node_modules/better-docs/category",
		"node_modules/better-docs/typescript",
		"plugins/markdown"
	],
	"opts": {
		"destination": "docs",
		"recurse": true,
		"verbose": true,
		"readme": "README.md",
		"template": "node_modules/better-docs"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"search": true,
		"better-docs": {
			"name": "GenUI React Starter Docs",
			"title": "GenUI React Starter Docs [replace this title in jsdoc.conf.json]",
			"navLinks": [
				{
					"label": "Github",
					"href": "https://github.com/devan-huapaya/genui-nextjs-starter"
				}
			]
		}
	}
}
